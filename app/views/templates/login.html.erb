<h3>Login</h3>
<p ng-repeat="error in errors">{{error}}</p>
<div class="container">
	<table>
		<tr><td>Email:</td><td><input type="text" ng-model="session.email" /></td></tr>
		<tr><td>Password:</td><td><input type="password" ng-model="session.password" /></td></tr>
		<tr><td></td><td><button class="btn btn-primary" ng-click="submit()">LogIn</button></td></tr>
	</table>
	<p>New User <a href="#sign_up">SignUp Now</a></p>
</div>
